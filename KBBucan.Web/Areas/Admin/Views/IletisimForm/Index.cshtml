@using KBBucan.Web.Models;
<head>
    <title>Admin - Kerim Berkay Buçan</title>
    <link rel="icon" type="image/x-icon" href="/images/favicon.png">

    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
</head>
<body>
    <div class="m-5">
        <div class="text-center  bg-warning m-5"><h2>Iletisim Formu Mesaj Kutusu</h2></div>
        <table border="1" class="table table-striped table-hover">
            <thead>
            <th>MesajID</th>
            <th>Ad Soyad</th>
            <th>Mail Adresi</th>
            <th>Telefon Numarası</th>
            <th>Konu</th>
            <th>Mesaj</th>
            <th>Proje Bütçesi</th>
            </thead>
            <tbody>
                @foreach (var iForm in ViewBag.IletisimFormlari as List<IletisimFormu>)
                {
                    <tr>
                        <td>@(iForm.MesajId)</td>
                        <td>@(iForm.AdSoyad)</td>
                        <td>@(iForm.MailAdres)</td>
                        <td>@(iForm.TelefonNumarasi)</td>
                        <td>@(iForm.Konu)</td>
                        <td>@(iForm.Mesaj)</td>
                        <td>@(iForm.ProjeButcesi)</td>


                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>